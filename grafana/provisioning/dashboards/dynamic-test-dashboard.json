{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": 1,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "postgres",
        "uid": "${datasource}"
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "cellOptions": {
              "type": "color-text"
            },
            "inspect": false
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "overall_result"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "pass": {
                        "color": "green",
                        "index": 0
                      },
                      "fail": {
                        "color": "red",
                        "index": 1
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byRegexp",
              "options": "/test_.*/"
            },
            "properties": [
              {
                "id": "custom.cellOptions",
                "value": {
                  "type": "color-background"
                }
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "pass": {
                        "color": "green",
                        "index": 0
                      },
                      "fail": {
                        "color": "red",
                        "index": 1
                      },
                      "Not tested": {
                        "color": "yellow",
                        "index": 2
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 20,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "cellHeight": "sm",
        "footer": {
          "countRows": false,
          "fields": "",
          "reducer": [
            "sum"
          ],
          "show": false
        },
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "start_time"
          }
        ]
      },
      "pluginVersion": "10.2.0",
      "targets": [
        {
          "datasource": {
            "type": "postgres",
            "uid": "${datasource}"
          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH test_data AS (\n  SELECT * FROM get_dynamic_test_summary(\n    NULL,\n    $__timeFrom()::timestamp,\n    $__timeTo()::timestamp\n  )\n)\nSELECT \n  unit_test_id,\n  car_number,\n  station_name,\n  overall_result,\n  start_time,\n  duration,\n  station_id,\n  (jsonb_each_text(dynamic_columns)).key as test_name,\n  (jsonb_each_text(dynamic_columns)).value::jsonb->>'value' as test_result\nFROM test_data\nORDER BY start_time DESC",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Dynamic Test Results",
      "transformations": [
        {
          "id": "extractFields",
          "options": {
            "format": "json",
            "jsonPaths": [
              {
                "path": "test_result"
              }
            ],
            "keepTime": false,
            "replace": false,
            "source": "test_result"
          }
        },
        {
          "id": "groupBy",
          "options": {
            "fields": {
              "car_number": {
                "aggregations": [],
                "operation": "groupby"
              },
              "duration": {
                "aggregations": [],
                "operation": "groupby"
              },
              "overall_result": {
                "aggregations": [],
                "operation": "groupby"
              },
              "start_time": {
                "aggregations": [],
                "operation": "groupby"
              },
              "station_id": {
                "aggregations": [],
                "operation": "groupby"
              },
              "station_name": {
                "aggregations": [],
                "operation": "groupby"
              },
              "test_name": {
                "aggregations": [],
                "operation": "groupby"
              },
              "test_result": {
                "aggregations": [
                  "first"
                ],
                "operation": "aggregate"
              },
              "unit_test_id": {
                "aggregations": [],
                "operation": "groupby"
              }
            }
          }
        },
        {
          "id": "pivot",
          "options": {
            "group": [
              "unit_test_id",
              "car_number",
              "station_name",
              "overall_result",
              "start_time",
              "duration",
              "station_id"
            ],
            "pivotBy": "test_name",
            "value": "test_result (first)"
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 38,
  "tags": [
    "test-results",
    "dynamic"
  ],
  "templating": {
    "list": [
      {
        "current": {
          "selected": false,
          "text": "PostgreSQL",
          "value": "PostgreSQL"
        },
        "hide": 0,
        "includeAll": false,
        "label": "Data Source",
        "multi": false,
        "name": "datasource",
        "options": [],
        "query": "postgres",
        "queryValue": "",
        "refresh": 1,
        "regex": "",
        "skipUrlSync": false,
        "type": "datasource"
      }
    ]
  },
  "time": {
    "from": "now-7d",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "",
  "title": "Dynamic Test Results Dashboard",
  "uid": "dynamic-test-results",
  "version": 1,
  "weekStart": ""
}
